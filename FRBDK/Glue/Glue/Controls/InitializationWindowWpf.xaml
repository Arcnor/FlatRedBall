<Window x:Class="GlueFormsCore.Controls.InitializationWindowWpf"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GlueFormsCore.Controls"
             mc:Ignorable="d" 
             Height="240" Width="550">
    <StackPanel Margin="12,0,12,12">
        <TextBlock x:Name="TopLevelLabel" TextWrapping="Wrap" FontSize="22">Loading Glue...</TextBlock>
        <TextBlock Margin="0,8,0,0" x:Name="SubLabel" TextWrapping="Wrap"></TextBlock>
    </StackPanel>
</Window>
